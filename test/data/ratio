{

pos1 = 0
pos2 = 0
pos3 = 0
pos4 = 0

var1 = 10
var2 = 1
var3 = 10
var4 = 1
var5 = 0
max_val = 1023

while(NR <= max_val)
    {	
	printf("%04d %04d %04d %04d %04d \x0d\x0a", $1 +pos1, $2 + pos2, $3 + pos3, $4 + pos4, $5 + var5)
	pos1 += var1 
	pos2 += var2
	pos3 += var3
	pos4 += var4
	if(pos1 > max_val || pos2 > max_val || pos3 > max_val || pos4 > max_val)
	{	    
	    exit
	}
    }
}
